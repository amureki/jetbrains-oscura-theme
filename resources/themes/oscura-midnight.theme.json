{
  "name": "Oscura Midnight",
  "author": "amureki",
  "dark": true,
  "editorScheme": "/colors/OscuraMidnight.xml",
  "colors": {
    "primaryBackground": "#0F0F14",
    "secondaryBackground": "#0B0B0F",
    "primaryForeground": "#E6E6E6",
    "secondaryForeground": "#868F97",
    "accentColor": "#E6E7A3",
    "selectionBackground": "#1A1A1F",
    "borderColor": "#32333B",
    "shadowColor": "#00000060"
  },
  "ui": {
    "*": {
      "background": "primaryBackground",
      "foreground": "primaryForeground",
      "infoForeground": "secondaryForeground",
      "selectionBackground": "selectionBackground",
      "selectionForeground": "primaryForeground",
      "selectionInactiveBackground": "secondaryBackground",
      "selectionBackgroundInactive": "secondaryBackground",
      "borderColor": "borderColor"
    },
    "ActionButton": {
      "hoverBackground": "#FFFFFF08",
      "pressedBackground": "#FFFFFF12"
    },
    "Button": {
      "arc": 4,
      "background": "#32333B",
      "foreground": "primaryForeground",
      "startBackground": "#32333B",
      "endBackground": "#32333B",
      "startBorderColor": "#32333B",
      "endBorderColor": "#32333B",
      "focusedBorderColor": "accentColor",
      "default": {
        "foreground": "#000000",
        "startBackground": "accentColor",
        "endBackground": "accentColor",
        "startBorderColor": "accentColor",
        "endBorderColor": "accentColor",
        "focusColor": "accentColor",
        "focusedBorderColor": "accentColor"
      }
    },
    "CompletionPopup": {
      "background": "secondaryBackground",
      "nonFocusedState": false,
      "selectionBackground": "selectionBackground",
      "selectionInactiveBackground": "selectionBackground",
      "matchForeground": "accentColor"
    },
    "Counter": {
      "background": "accentColor",
      "foreground": "#000000"
    },
    "Editor": {
      "background": "primaryBackground",
      "foreground": "primaryForeground",
      "shortcutForeground": "accentColor"
    },
    "EditorTabs": {
      "background": "primaryBackground",
      "borderColor": "borderColor",
      "underlinedTabBackground": "primaryBackground",
      "inactiveUnderlineColor": "borderColor",
      "underlineColor": "accentColor",
      "underlineHeight": 2
    },
    "Link": {
      "activeForeground": "accentColor",
      "hoverForeground": "accentColor",
      "pressedForeground": "accentColor",
      "visitedForeground": "accentColor",
      "secondaryForeground": "secondaryForeground"
    },
    "Notification": {
      "background": "secondaryBackground",
      "borderColor": "borderColor",
      "errorBackground": "secondaryBackground",
      "errorBorderColor": "#FF5C5C",
      "warningBackground": "secondaryBackground",
      "warningBorderColor": "#FFA16C",
      "informativeBackground": "secondaryBackground",
      "informativeBorderColor": "accentColor"
    },
    "Popup": {
      "Header": {
        "activeBackground": "secondaryBackground",
        "inactiveBackground": "primaryBackground"
      },
      "borderColor": "borderColor",
      "inactiveBorderColor": "borderColor",
      "innerBorderColor": "borderColor"
    },
    "ProgressBar": {
      "background": "accentColor",
      "trackColor": "#32333B",
      "progressColor": "accentColor",
      "indeterminateStartColor": "accentColor",
      "indeterminateEndColor": "#32333B",
      "failedColor": "#FF5C5C",
      "failedEndColor": "#FF5C5C",
      "passedColor": "#4EBE96",
      "passedEndColor": "#4EBE96"
    },
    "ScrollBar": {
      "track": "#00000000",
      "thumb": "#FFFFFF12",
      "thumbColor": "#FFFFFF12",
      "hoverThumbColor": "#FFFFFF20",
      "thumbBorderColor": "#00000000",
      "hoverThumbBorderColor": "#00000000"
    },
    "SearchEverywhere": {
      "Tab": {
        "selectedBackground": "selectionBackground",
        "selectedForeground": "primaryForeground"
      },
      "SearchField": {
        "background": "primaryBackground",
        "borderColor": "borderColor"
      }
    },
    "StatusBar": {
      "borderColor": "borderColor",
      "hoverBackground": "#FFFFFF08"
    },
    "TabbedPane": {
      "underlineColor": "accentColor",
      "disabledUnderlineColor": "#32333B",
      "contentAreaColor": "borderColor"
    },
    "ToolWindow": {
      "Header": {
        "background": "primaryBackground",
        "borderColor": "borderColor",
        "inactiveBackground": "primaryBackground"
      },
      "HeaderTab": {
        "selectedBackground": "selectionBackground",
        "hoverBackground": "#FFFFFF08",
        "selectedInactiveBackground": "selectionBackground"
      }
    },
    "Tree": {
      "background": "primaryBackground",
      "selectionBackground": "selectionBackground",
      "modifiedItemForeground": "accentColor"
    },
    "ValidationTooltip": {
      "errorBackground": "#FF5C5C",
      "errorBorderColor": "#FF5C5C",
      "warningBackground": "#FFA16C",
      "warningBorderColor": "#FFA16C"
    },
    "VersionControl": {
      "Log.Commit": {
        "currentBranchBackground": "selectionBackground",
        "hoveredBackground": "#FFFFFF08"
      },
      "RefLabel": {
        "backgroundBase": "accentColor",
        "foreground": "#000000"
      }
    },
    "WelcomeScreen": {
      "Projects.selectionBackground": "selectionBackground",
      "Projects.selectionInactiveBackground": "selectionBackground"
    }
  }
} 